URxvt.perl-ext-common:  default,matcher,clipboard
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.url-launcher:     firefox
URxvt.matcher.button:   1
URxvt.font: xft:Inconsolata for Powerline:pixelsize=22,xft:Monospace:pixelsize=18:antialias=false
URxvt.boldFont: xft:Inconsolata for Powerline:bold:pixelsize=22,xft:Monospace:pixelsize=18:antialias=false
URxvt.letterSpace: 0
URxvt.lineSpace: 6
urxvt*termName: rxvt
urxvt*scrollBar:false
urxvt*cursorBlink:  true
urxvt*cursorUnderline: true
urxvt*cursorColor:  #ececec
URxvt.internalBorder:   10
URxvt*saveLines: 12000

urxvt*depth: 32
urxvt.background: [50]#000000

URxvt*foreground: #E8E8E8
URxvt*background: #000000

!! Gobal Terms color
! black + red
*color0:      #000000
*color1:      #cd4040
! green + yellow
*color2:      #8eae71
*color3:      #d2b48c
! blue + purple
*color4:      #5f7b8a
*color5:      #bdbda4
! cyan + white
*color6:      #686868
*color7:      #bfbfbf
! bright-black + bright-red
*color8:      #000000
*color9:      #ee8080
! bright-green + bright-yellow
*color10:     #80dd80
*color11:     #cdcdc1
! bright-blue + bright-purple
*color12:     #9f9faf
*color13:     #dfdfd8
! bright-cyan + bright-white
*color14:     #8f9faf
*color15:     #ffffff
